{
    "name": "lumilock/lumilock",
    "description": "A package to create an admin panel API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Perrin Thibaud",
            "email": "thibaud.perrin@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "league/flysystem": " ~1.0",
        "tymon/jwt-auth": "^1.0",
        "laravel/legacy-factories": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "lumilock\\lumilock\\": "/"
        },
        "files": [
            "app/Libraries/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "lumilock\\lumilock\\Providers\\LumilockServiceProvider"
            ],
            "aliases": {
                "lumilock": "lumilock\\lumilock\\Facades\\lumilock"
            }
        }
    }
}
